{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "browser": true,
      // "jquery": true,
      "node": true,
      "jasmine": false,
    },
    "rules": {
        "semi": 1,
        "indent": ["warn", 2],
        "no-console": "off",
        "no-unused-vars": "warn"
    }
}
